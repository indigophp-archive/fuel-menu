{
    "name": "indigophp/fuel-menu",
    "type": "fuel-package",
    "description": "Fuel wrapper around Knp Menu package",
    "license": "MIT",
    "keywords": ["menu"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "composer/installers": "~1.0",
        "indigophp/fuel-dependency": "dev-develop",
        "knplabs/knp-menu": "~2.0.0"
    },
    "autoload": {
        "psr-0": {
            "Knp\\Menu\\": "lib/"
        },
        "psr-4": {
            "Indigo\\Fuel\\Menu\\": "classes/"
        }
    },
    "extra": {
        "installer-name": "menu"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
